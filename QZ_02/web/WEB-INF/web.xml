<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.cn.web.controller.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.cn.web.controller.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UI_register</servlet-name>
        <servlet-class>com.cn.web.UI.UI_Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UI_register</servlet-name>
        <url-pattern>/UI_register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UI_login</servlet-name>
        <servlet-class>com.cn.web.UI.UI_Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UI_login</servlet-name>
        <url-pattern>/UI_login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>picture</servlet-name>
        <servlet-class>com.cn.servlet.sjpicture</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>picture</servlet-name>
        <url-pattern>/sjpicture</url-pattern>
    </servlet-mapping>

</web-app>